# Tempo Configuration
# Copy this file to .env and fill in your actual values

# Required: Authentication token for the API service
TAP_TEMPO_AUTH_TOKEN=your_auth_token_here

# Optional: The earliest record date to sync (ISO format)
TAP_TEMPO_START_DATE=2023-01-01T00:00:00Z

# Optional: API URL (defaults to https://api.tempo.io/4)
TAP_TEMPO_API_URL=https://api.tempo.io/4

# Optional: Custom User-Agent header
TAP_TEMPO_USER_AGENT=tap-tempo/1.0.0
